{
  "name": "@pmp/api",
  "version": "1.0.0",
  "description": "PMP Study Application API Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:ebook": "ts-node prisma/seed-ebook.ts",
    "seed:flashcards:json": "ts-node prisma/seed-flashcards-json.ts",
    "seed:questions:json": "ts-node prisma/seed-questions-json.ts",
    "db:studio": "prisma studio",
    "export:openapi": "ts-node scripts/export-openapi-spec.ts",
    "generate:postman": "ts-node scripts/generate-postman-collection.ts",
    "docs:export": "npm run export:openapi && npm run generate:postman"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.958.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.958.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.67.3",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.208.0",
    "@opentelemetry/exporter-trace-otlp-proto": "0.208.0",
    "@opentelemetry/instrumentation-express": "0.57.1",
    "@opentelemetry/instrumentation-http": "0.208.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-node": "0.208.0",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "@pmp/shared": "1.0.0",
    "@prisma/client": "^7.2.0",
    "@types/aws-sdk": "^2.7.4",
    "aws-sdk": "^2.1693.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3",
    "stripe": "^20.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-cloudwatch": "^6.3.0",
    "yamljs": "^0.3.0",
    "zod": "^4.3.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@opentelemetry/exporter-trace-otlp-grpc": "0.208.0",
    "@opentelemetry/instrumentation-fetch": "^0.208.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^25.0.3",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "@types/yamljs": "^0.2.34",
    "fast-check": "^4.5.3",
    "jest": "^30.2.0",
    "openapi-types": "^12.1.3",
    "prisma": "^7.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}