{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    ".eslintrc.js",
    "*.js",
    "*.ts",
    "*.tsx",
    "packages/**/*.ts",
    "packages/**/*.tsx",
    "packages/**/*.js"
  ],
  "exclude": ["node_modules", "dist", "coverage", "packages/**/dist", "packages/**/.next"]
}
